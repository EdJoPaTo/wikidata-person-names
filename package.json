{
	"name": "wikidata-person-names",
	"version": "1.1.0",
	"description": "Set of given and last names pulled from Wikidata",
	"keywords": [
		"data",
		"dataset",
		"datasets",
		"family",
		"female",
		"first",
		"firstname",
		"given",
		"human",
		"last",
		"lastname",
		"male",
		"name",
		"names",
		"people",
		"person",
		"personal",
		"sample",
		"unisex",
		"wikidata"
	],
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-person-names",
	"author": {
		"name": "EdJoPaTo",
		"email": "wikidata-person-names-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"start": "npx tsc && node dist/index.js",
		"test": "tsc --sourceMap && xo"
	},
	"type": "commonjs",
	"engines": {
		"node": ">=12"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/node": "^18.0.0",
		"del-cli": "^4.0.0",
		"typescript": "^4.4.4",
		"wikidata-sdk-got": "^2.0.1",
		"xo": "^0.50.0"
	},
	"files": [
		"index.*"
	],
	"main": "index.js",
	"types": "index.d.ts",
	"xo": {
		"rules": {
			"@typescript-eslint/naming-convention": "off",
			"unicorn/prefer-module": "off",
			"unicorn/prefer-node-protocol": "off"
		}
	}
}
