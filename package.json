{
	"name": "wikidata-person-names",
	"version": "1.2.1",
	"description": "Set of given and last names pulled from Wikidata",
	"keywords": [
		"data",
		"dataset",
		"datasets",
		"family",
		"female",
		"first",
		"firstname",
		"given",
		"human",
		"last",
		"lastname",
		"male",
		"name",
		"names",
		"people",
		"person",
		"personal",
		"sample",
		"unisex",
		"wikidata"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/EdJoPaTo/wikidata-person-names"
	},
	"author": {
		"name": "EdJoPaTo",
		"email": "wikidata-person-names-npm-package@edjopato.de",
		"url": "https://edjopato.de"
	},
	"scripts": {
		"build": "./build.sh",
		"prepack": "npm run build",
		"update-names": "deno run -A update-names.ts",
		"test": "node node-test.cjs"
	},
	"type": "commonjs",
	"engines": {
		"node": ">=0.8"
	},
	"files": [
		"index.*js",
		"index.d.ts"
	],
	"main": "./index.cjs",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.mjs",
			"require": "./index.cjs"
		}
	},
	"types": "./index.d.ts"
}
